      <default>1000</default>
    </key>
    <key name="default-height" type="i">
      <default>700</default>
    </key>
    <key name="maximized" type="b">
      <default>false</default>
    </key>
    <key name="stack" type="s">
      <default>"Browse"</default>
    </key>
    <key name="safe-mode" type="b">
      <default>true</default>
    </key>
    <key name="deleted-posts" type="b">
      <default>false</default>
    </key>
    <key name="pending-posts" type="b">
      <default>false</default>
    </key>
    <key name="blacklist" type="as">
      <default>[]</default>
    </key>
    <key name="posts-per-page" type="i">
      <default>60</default>
    </key>
    <key name="thumbnail-size" type="i">
      <default>2</default>
    </key>
    <key name="save-files" type="b">
      <default>false</default>
    </key>
    <key name="new-tab-query" type="s">
      <default>"scenery"</default>
    </key>
    <key name="saved-searches" type="as">
      <default>[]</default>
    </key>
    <key name="sort" type="s">
      <default>"last-added"</default>
    </key>
 </schema>
</schemalist>
